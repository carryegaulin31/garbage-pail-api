  "devDependencies": {
    "env-cmd": "^10.1.0",
    "eslint": "^7.13.0",
    "nodemon": "^2.0.6",
    "chai": "^4.2.0",
    "mocha": "^8.2.1",
    "sinon": "^9.2.1",
    "sinon-chai": "^3.5.0"
  },
  "dependencies": {
